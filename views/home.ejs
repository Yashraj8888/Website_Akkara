        <div class="shareus">
            <section id="contact">
                <h1 id="hi">Share with us</h1>

                <form action="/Contact" method="POST">

                    <div class="input-contactus" data-aos="fade-left" data-aos-delay="200">
                        <label for="name">Name:</label><br>
                        <input type="text" id="name" name="name" placeholder="Enter your name">
                    </div>
                    <div class="input-contactus" data-aos="fade-left" data-aos-delay="200">
                        <label for="email">Email:</label><br>
                        <input type="email" id="email" name="email" placeholder="Enter your Email">
                    </div>
                    <div class="input-contactus" data-aos="fade-left" data-aos-delay="200">
                        <label for="number">phone numbe:</label><br>
                        <input type="number" id="number" name="number" placeholder="Enter your Contact number">
                    </div>
                    <div class="input-contactus" data-aos="fade-left" data-aos-delay="200">
                        <label for="mssg">Suggestions:</label><br>
                        <textarea type="text" columns="30" rows="10" name="message" id="mssg"></textarea>
                    </div>
                    <button class="btn" data-aos="fade-left" data-aos-delay="200">Submit</button>


                </form>
            </section>
        </div>
    </section>








